package lab72;


//Program to use this keyword to compare two student objects
public class Student {
 String name;
 int id;

 Student(String name, int id) {
     this.name = name;
     this.id = id;
 }

 boolean isSame(Student s) {
     return this.id == s.id;
 }

 public static void main(String[] args) {
     Student s1 = new Student("Ravi", 101);
     Student s2 = new Student("Ravi", 101);
     System.out.println(s1.isSame(s2));
 }
}
