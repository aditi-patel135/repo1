package paperpractise;
//method overloading Write a Calculator class with overloaded methods named add() that: 
public class MCalculator {
static int add(int a,int b) {
	return a+b; }
static int add(int a,int b,int c) {
	return a+b+c;}
static double add(double a,double b){
	return a+b;
}
public static void main(String[] args) {
	 System.out.println(add(1,61));
	 System.out.println(add(1,5,20));
	 System.out.println(add(45.5,13.5));

}
}
