package lab72;

abstract class Animal {
    abstract void makeSound();

    void sleep() {
        System.out.println("Sleeping...");
    }
}

class Dog extends Animal {
    void makeSound() {
        System.out.println("Woof Woof!");
    }
}

public class print {
    public static void main(String[] args) {
        Animal ki = new Dog();   
      ki.makeSound();          
        ki.sleep();              
    }
}

